{
  "name": "@chainspin/cli",
  "version": "0.11.0",
  "description": "Command-line tool for ChainSpin framework.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "chainspin": "./bin/chainspin"
  },
  "scripts": {
    "clean": "rm -Rf ./dist; rm -Rf ./build",
    "build": "npm run clean; tsc",
    "lint": "tslint 'src/**/*.ts?(x)'",
    "prepublishOnly": "npm run build"
  },
  "chainspin": {
    "sandboxHost": "127.0.0.1",
    "sandboxPort": 8545,
    "solcSrc": "src/contracts",
    "solcDist": "dist/contracts",
    "testRoot": "dist/tests",
    "testTimeout": 60000
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/inquirer": "0.0.41",
    "@types/mocha": "^5.2.1",
    "@types/node": "^10.3.2",
    "chai": "^4.1.2",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  },
  "dependencies": {
    "@chainspin/compiler": "^0.11.0",
    "@chainspin/init": "^0.11.0",
    "@chainspin/sandbox": "^0.11.0",
    "@chainspin/test": "^0.11.0",
    "@chainspin/utils": "^0.11.0",
    "commander": "^2.15.1",
    "inquirer": "^6.0.0",
    "ts-node": "6.1.0"
  }
}
