{
  "name": "@chainspin/cli",
  "version": "0.16.1",
  "description": "Command-line tool for ChainSpin framework.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "chainspin": "./bin/chainspin"
  },
  "scripts": {
    "clean": "rm -Rf ./dist; rm -Rf ./build",
    "transpile": "npm run clean; tsc",
    "lint": "tslint 'src/**/*.ts?(x)'",
    "prepublishOnly": "npm run build",
    "test": "npm run transpile && ava"
  },
  "ava": {
    "compileEnhancements": false,
    "files": [
      "dist/tests/*.test.js",
      "dist/tests/**/*.test.js"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@types/args": "^3.0.0",
    "@types/inquirer": "^0.0.41",
    "@types/node": "^10.3.2",
    "@types/yargs": "11.0.0",
    "ava": "^0.25.0",
    "ts-node": "^6.1.1",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1",
    "web3": "^1.0.0-beta.34"
  },
  "peerDependencies": {
    "web3": "^1.0.0-beta.34"
  },
  "dependencies": {
    "@chainspin/compiler": "latest",
    "@chainspin/init": "latest",
    "@chainspin/sandbox": "latest",
    "@chainspin/spec": "latest",
    "args": "^5.0.0",
    "inquirer": "^6.0.0",
    "typedarray-to-buffer": "3.1.5",
    "yargs": "11.0.0"
  }
}
